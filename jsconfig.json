{
  "compilerOptions": {
    "target": "ES2022",
    "lib": [
      "dom",
      "dom.iterable",
      "es2022"
    ],
    "allowJs": true,
    "checkJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "isolatedModules": false,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "ignoreDeprecations": "6.0",
    "strict": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "noImplicitReturns": false,
    "noFallthroughCasesInSwitch": false,
    "noImplicitAny": false,
    "noPropertyAccessFromIndexSignature": false,
    "noUncheckedIndexedAccess": false,
    //i want suppress missing attribute errors
    "paths": {
      "@/*": [
        "*"
      ],
      "@/components/*": [
        "components/*"
      ],
      "@/utils/*": [
        "utils/*"
      ]
    }
  },
  "include": [
    "src/**/*.js",
    "src/**/*.jsx",
    "src/features/auth/users/UsersPage.فsx",
    "src/features/auth/register/const/colors.فs"
  ],
  "exclude": [
    "node_modules",
    "build",
    "dist"
  ]
}